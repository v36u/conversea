{
  "name": "conversea",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prisma-generate": "prisma generate",
    "prisma-migrate": "prisma migrate deploy",
    "prisma-migrate-dev": "prisma migrate dev",
    "prisma-reset": "prisma migrate reset",
    "prisma-seed": "prisma db seed",
    "prisma-studio": "prisma studio",
    "postinstall": "pnpm prisma-generate",
    "lint": "eslint --ext \".js,.ts,.tsx,.json\" --report-unused-disable-directives --report-unused-disable-directives .",
    "predev": "",
    "dev": "cross-env NODE_ENV=development run-s dev:* --print-label",
    "dev:prisma": "run-s prisma-migrate prisma-generate prisma-seed",
    "dev:next": "next dev",
    "build": "cross-env NODE_ENV=production run-s build:* --print-label",
    "build:prisma": "run-s generate-postgres prisma-migrate",
    "build:next": "next build",
    "start": "next start",
    "test": "cross-env NODE_ENV=test run-s test:* --print-label",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test 'next dev' http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "@tanstack/react-query": "^4.3.8",
    "@trpc/client": "^10.12.0",
    "@trpc/next": "^10.12.0",
    "@trpc/react-query": "^10.12.0",
    "@trpc/server": "^10.12.0",
    "clsx": "^1.1.1",
    "next": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@types/dotenv-flow": "^3.2.0",
    "@types/next": "^9.0.0",
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.10.1",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.9.0",
    "typescript": "^4.9.5",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
